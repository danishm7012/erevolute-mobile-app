{
  "expo": {
    "name": "eRevolute",
    "slug": "erevolute",
    "description": "The Best Course Design for you crafted by professionals",
    "version": "1.0.0",
    "privacy": "public",
    "sdkVersion": "40.0.0",
    "platforms": ["ios", "android"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e4d90"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "org.erevolute",
      "buildNumber": "1.0.0",
      "supportsTablet": false
    },
    "android": {
      "package": "org.erevolute",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#2e4d90"
      },
      "permissions": ["READ_EXTERNAL_STORAGE"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
